{
    "name": "ecourty/ethereum-php",
    "description": "Interact with an Ethereum node via JSON-RPC using PHP",
    "type": "library",
    "homepage": "https://github.com/EdouardCourty/ethereum-php",
    "keywords": [
        "ethereum",
        "json-rpc",
        "php"
    ],
    "prefer-stable": true,
    "require": {
        "php": "^8.3",
        "ext-bcmath": "*",
        "symfony/http-client": "^7.2"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.0",
        "phpunit/phpunit": "^11.5",
        "friendsofphp/php-cs-fixer": "^3.65"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "EthereumPHP\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EthereumPHP\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Edouard Courty",
            "email": "edouard.courty2@gmail.com",
            "homepage": "https://github.com/EdouardCourty",
            "role": "Author"
        }
    ]
}
